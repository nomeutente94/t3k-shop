<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee">
  <display-name>t3k-shop</display-name>
  <servlet>
    <servlet-name>search</servlet-name>
    <servlet-class>control.SearchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>search</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegServlet</servlet-name>
    <servlet-class>control.RegServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegServlet</servlet-name>
    <url-pattern>/RegServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogServlet</servlet-name>
    <servlet-class>control.LogServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogServlet</servlet-name>
    <url-pattern>/LogServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>submitProduct</servlet-name>
    <servlet-class>control.submitProduct</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>submitProduct</servlet-name>
    <url-pattern>/submitProduct</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>product</servlet-name>
    <servlet-class>control.ProductServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>product</servlet-name>
    <url-pattern>/product</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>fillDatabase</servlet-name>
    <servlet-class>control.fillDatabase</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fillDatabase</servlet-name>
    <url-pattern>/fillDatabase</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addCart</servlet-name>
    <servlet-class>control.addCart</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addCart</servlet-name>
    <url-pattern>/addCart</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>removeCart</servlet-name>
    <servlet-class>control.removeCart</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>removeCart</servlet-name>
    <url-pattern>/removeCart</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>addWishlist</servlet-name>
    <servlet-class>control.addWishlist</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addWishlist</servlet-name>
    <url-pattern>/addWishlist</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>removeWishlist</servlet-name>
    <servlet-class>control.removeWishlist</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>removeWishlist</servlet-name>
    <url-pattern>/removeWishlist</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>Logout</servlet-name>
  	<servlet-class>control.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>